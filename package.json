{
  "name": "orb-ui",
  "version": "0.1.0",
  "description": "Beautiful animated UI components for voice AI agents",
  "type": "module",
  "module": "./dist/orb-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/orb-ui.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters.js",
      "types": "./dist/adapters/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.0",
    "vite": "^5.3.0",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.6.0"
  },
  "keywords": ["voice", "ai", "react", "animation", "component", "vapi", "elevenlabs"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexanderqchen/orb-ui.git"
  }
}
